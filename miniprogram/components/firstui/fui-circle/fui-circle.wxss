.fui-circle__wrap {
  position: relative
}

.fui-circle__inner {
  width: 100%;
  display: flex;
  z-index: 10;
  text-align: center;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.fui-circle__canvas {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 5;
}